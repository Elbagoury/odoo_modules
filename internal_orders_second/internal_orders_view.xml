<?xml version="1.0" encoding="UTF-8"?>
	<openerp>
		<data>
			<record id="internal_order_second" model="ir.ui.view">
				<field name="name">internal.order.second.form</field>
				<field name="model">purchase.order</field>
				<field name="inherit_id" ref="purchase.purchase_order_form"/>
				<field name="arch" type="xml">
					<xpath expr="//field[@name='company_id']" position="after">
						<field name="transfer_remotely"/>
						<field name="remote_order_id" attrs="{'invisible': [('remote_order_id', '=', False)]}"/>
						<button name="transfer_orderX" type="button" string="Transfer" />

					</xpath>

        </field>
      </record>

      <menuitem id="menu_ims" parent="base.menu_administration" name="Internal moves 2" sequence="3" groups="base.group_no_one"/>


      <record model="ir.actions.act_window" id="action_ims_conf">
    		<field name="name">Configuration</field>
    		<field name="res_model">internal.moves</field>
    		<field name="view_model">tree, form</field>
    	</record>
		  <menuitem id="menu_ims_conf" parent="menu_ims" name="Configuration" action="action_ims_conf"/>
    </data>
  </openerp>
